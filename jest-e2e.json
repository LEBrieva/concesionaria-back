{
  "preset": "ts-jest",
  "testEnvironment": "node",
  "testMatch": [
    "**/src/modules/autos/tests/**/*.test.ts"
  ],
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  },
  "moduleFileExtensions": [
    "ts",
    "js",
    "json",
    "node"
  ],
  "collectCoverage": true,
  "coverageDirectory": "coverage",
  "testTimeout": 30000
}